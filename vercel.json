{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(styles\\.css|main\\.js|favicon\\.ico|404\\.html)",
      "dest": "/public/$1"
    },
    {
      "src": "/([^/]+)",
      "dest": "/public/index.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
